{
  "name": "image-labeler",
  "version": "0.0.4",
  "description": "Autosuggested labels for images and video",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.min.js",
  "module": "dist/index.mjs",
  "scripts": {
    "test": "karma start karma.config.js --auto-watch",
    "test:ci": "karma start karma.config.js --single-run --no-auto-watch",
    "build": "rimraf dist && microbundle --name ImageLabeler",
    "dev": "rimraf dist && microbundle watch --name ImageLabeler"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/image-labeler.git"
  },
  "author": "Kevin Scott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thekevinscott/image-labeler/issues"
  },
  "homepage": "https://github.com/thekevinscott/image-labeler#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.4",
    "canvas": "^2.3.1",
    "chai": "^4.2.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "^4.0.0",
    "microbundle": "^0.9.0",
    "mocha": "^6.0.0",
    "mocha-sinon": "^2.1.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.15.2"
  }
}
